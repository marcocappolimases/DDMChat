<Window x:Class="MASES.S4I.ChatUI.ConfigurationWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MASES.S4I.ChatUI"
        mc:Ignorable="d"
        Title="Configuration" Height="300" Width="300">
    <Grid>
        <StackPanel>
            <Label Content="Name"></Label>
            <TextBox Text="{Binding Profile.Name}"></TextBox>
            <Label Content="Last Name"></Label>
            <TextBox Text="{Binding Profile.LastName}"></TextBox>
            <Button Content="Save" Click="Save_button"></Button>
        </StackPanel>
    </Grid>
</Window>
